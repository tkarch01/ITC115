package business;

import ui.Console;

public class Player1 extends Player {

	public Player1() {
		// TODO Auto-generated constructor stub
	}	

	public String generateRoshambo(String choice) {
		
		switch(choice){
		case "r":
			choice =  Roshambo.ROCK.toString()	;		
			break;
		case "p":
			choice =  Roshambo.ROCK.toString()	;	
			break;
		case "s":
			choice =  Roshambo.ROCK.toString()	;	
			break;
		}
		return choice;
	}
}